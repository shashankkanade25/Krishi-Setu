            display: grid;
            grid-template-columns: 1fr 1fr;
            gap: 20px;
        }

        .farmer-img {
            width: 100%;
            height: 250px;
            background-color: #8bc34a;
            border-radius: 15px;
            object-fit: cover;
        }

        /* Reviews Section */
        .reviews {
            padding: 80px 20px;
            background-color: #f5f5dc;
        }

        .reviews-grid {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(400px, 1fr));
            gap: 40px;
            max-width: 1200px;
            margin: 0 auto;
        }

        .review {
            display: flex;
            gap: 20px;
            align-items: flex-start;
        }

        .review-avatar {
            width: 80px;
            height: 80px;
            background-color: #8bc34a;
            border-radius: 50%;
            flex-shrink: 0;
        }

        .review-content p {
            font-size: 16px;
            line-height: 1.6;
            margin-bottom: 15px;
            color: #555;
        }

        .review-author {
            font-weight: bold;
            color: #333;
        }

        /* Join Community Section */
        .join-community {
            padding: 80px 20px;
            background-color: #8bc34a;
            text-align: center;
            color: #000;
        }

        .join-community h2 {
            font-size: 42px;
            margin-bottom: 30px;
            font-weight: bold;
        }

        .join-community p {
            font-size: 18px;
            margin-bottom: 15px;
        }

        .join-community .role {
            font-weight: bold;
            margin-top: 20px;
        }

        .join-btn {
            background-color: #9ec158;
            color: #000;
            padding: 15px 40px;
            border-radius: 25px;
            border: none;
            cursor: pointer;
            font-size: 18px;
            font-weight: 600;
            margin-top: 30px;
            text-decoration: none;
            display: inline-block;
        }

        .join-btn:hover {
            background-color: #b5d17b;
        }

        /* Footer */
        footer {
            background-color: #f5f5dc;
            padding: 60px 20px 20px;
            border-top: 1px solid #ddd;
        }

        .footer-content {
            text-align: center;
            max-width: 800px;
            margin: 0 auto;
        }

        .footer-content h2 {
            font-size: 32px;
            margin-bottom: 20px;
        }

        .footer-content h3 {
            font-size: 28px;
            margin-bottom: 30px;
            line-height: 1.5;
        }

        .contact-info {
            margin: 30px 0;
        }

        .contact-info p {
            margin: 8px 0;
            color: #555;
        }

        .social-icons {
            display: flex;
            gap: 15px;
            justify-content: center;
            margin: 30px 0;
        }

        .social-icon {
            width: 45px;
            height: 45px;
            background-color: #8bc34a;
            border-radius: 50%;
            display: flex;
            align-items: center;
            justify-content: center;
            color: white;
            text-decoration: none;
            font-size: 20px;
        }

        .copyright {
            margin-top: 40px;
            padding-top: 20px;
            border-top: 1px solid #ddd;
            display: flex;
            justify-content: space-between;
            color: #555;
        }

        .back-to-top {
            color: #555;
            text-decoration: none;
        }

        @media (max-width: 1024px) {
            .search-container {
                max-width: 300px;
            }

            .nav-right {
                gap: 15px;
            }

            .icon-link span:not(.icon) {
                display: none;
            }

            .categories-grid {
                grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
            }

            .products-grid {
                grid-template-columns: repeat(auto-fit, minmax(280px, 1fr));
            }
        }

        @media (max-width: 768px) {
            .nav-content {
                flex-wrap: wrap;
            }

            .search-container {
                order: 3;
                max-width: 100%;
                flex: 1 1 100%;
                margin-top: 10px;
            }

            .nav-right {
                gap: 15px;
            }

            .nav-link:not(.icon-link) {
                display: none;
            }

            .categories-grid {
                grid-template-columns: 1fr;
            }

            .products-grid {
                grid-template-columns: 1fr;
            }

            .fresh-produce-title h2 {
                font-size: 36px;
            }

            .steps {
                flex-direction: column;
            }

            .arrow {
                transform: rotate(90deg);
            }

            .farmers-content {
                flex-direction: column;
            }

            .reviews-grid {
                grid-template-columns: 1fr;
            }

            .hero h1 {
                font-size: 32px;
            }

            .section-title {
                font-size: 32px;
            }

            .copyright {
                flex-direction: column;
                gap: 10px;
                text-align: center;
            }
        }
    </style>
</head>
